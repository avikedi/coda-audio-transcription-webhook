# Django Configuration
DJANGO_SECRET_KEY=your-secret-key-here-change-in-production
DEBUG=False
ALLOWED_HOSTS=localhost,127.0.0.1,your-domain.com

# Redis/Celery Configuration
REDIS_URL=redis://localhost:6379/0

# Coda API Configuration
CODA_API_KEY=your_coda_api_key_here
CODA_DOC_ID=your_document_id_here
CODA_TABLE_ID=your_table_id_here

# Column names in your Coda table
COLUMN_AUDIO_URL=Audio File URL
COLUMN_STATUS=Status
COLUMN_TRANSCRIPT=Transcript
COLUMN_SUMMARY=Summary
COLUMN_PROCESSED_DATE=Processed Date

# Whisper Model Configuration (tiny, base, small, medium, large)
WHISPER_MODEL=tiny

# Processing Configuration
TEMP_DOWNLOAD_DIR=/tmp/audio

# Webhook Security (optional - recommended for production)
WEBHOOK_SECRET=your-webhook-secret-token-here
